  },
  "dependencies": {},
  "devDependencies": {
    "@taiyuuki/eslint-config-vue-unimport": "^0.0.5",
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "jsnes": "^1.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vitest": "^0.25.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.9"
  }
