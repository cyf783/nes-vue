{
  "name": "nes-vue",
  "version": "1.5.0",
  "description": "A NES(FC) emulator component for Vue 3",
  "module": "./dist/nes-vue.es.js",
  "main": "./dist/nes-vue.umd.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/nes-vue.es.js",
      "require": "./dist/nes-vue.umd.js"
    }
  },
  "keywords": [
    "fc",
    "nes",
    "jsnes",
    "vue",
    "nes emulator",
    "fc emulator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/taiyuuki/nes-vue.git"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./ --fix",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "patch": "bump patch",
    "minor": "bump minor",
    "publish": "pnpm publish --registry https://registry.npmjs.org"
  },
  "dependencies": {
    "@taiyuuki/eslint-config-vue": "^0.0.10",
    "@taiyuuki/utils": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "^2.27.5",
    "jsnes": "^1.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^3.2.7",
    "vite-plugin-dts": "^1.7.3",
    "vitest": "^0.25.8",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  }
}
